<%= render 'store/sidebar', store: @store %>

<div id="center" class="span9 shadow-box">
  <div class="span10">
    <%= render 'layouts/flash', flash: flash %>
    <h4><%= @store.name %> Summary</h4>
  </div>

  <div class="span2">
    <div class="input-append date" id="datepicker" data-date="2012-12-12" data-date-format="yyyy-mm-dd">
      <input size="16" type="text" value="2012-12-12" class="input-small" readonly>
      <span class="add-on"><i class="icon-calendar"></i></span>
    </div>
  </div>  
</div>
